<UserControl x:Class="SukiUI.Demo.Features.ControlsLibrary.Dialogs.StandardDialog"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <StackPanel Spacing="15">
        <TextBlock FontSize="18"
                   FontWeight="DemiBold"
				   Text="出现错误日志如下：" />
		<TextBlock x:Name="DialogContent" FontSize="15" HorizontalAlignment="Center" TextWrapping="Wrap"></TextBlock>
        <Button Click="Button_OnClick" Content="OK" />
    </StackPanel>
</UserControl>
