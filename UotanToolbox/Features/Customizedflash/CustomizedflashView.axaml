<UserControl x:Class="UotanToolbox.Features.Customizedflash.CustomizedflashView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:avalonia="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:customizedflash="clr-namespace:UotanToolbox.Features.Customizedflash"
			 xmlns:suki="clr-namespace:SukiUI.Controls;assembly=SukiUI"
             xmlns:theme="clr-namespace:SukiUI.Theme;assembly=SukiUI"
			 d:DesignWidth="1024"
             x:DataType="customizedflash:CustomizedflashViewModel"
             mc:Ignorable="d">
	<ScrollViewer VerticalScrollBarVisibility="Hidden">
		<WrapPanel Margin="15"
                   theme:WrapPanelExtensions.AnimatedScroll="True"
                   Orientation="Horizontal">
			<!-- Fastboot -->
			<suki:GlassCard Width="960"
							Height="240"
                            Margin="10"
                            VerticalAlignment="Top">
				<suki:BusyArea  Name="BusyTXTFlash"
								BusyText="执行中 ...">
					<Grid>
						<StackPanel Spacing="30">
							<StackPanel>
								<TextBlock HorizontalAlignment="Left"
										   VerticalAlignment="Top"
										   FontWeight="DemiBold"
										   Text="预置刷入" />
								<StackPanel Orientation="Horizontal" Spacing="20">
									<StackPanel>
										<StackPanel Margin="0" Orientation="Horizontal">
											<TextBlock Width="50"
													   Margin="8,0,0,0"
													   VerticalAlignment="Center"
													   FontSize="13"
													   Foreground="{DynamicResource SukiLowText}"
													   Text="System" />
											<TextBox Width="270"
													 theme:TextBoxExtensions.AddDeleteButton="False"/>
											<Button Height="35"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												文件
											</Button>
											<Button Height="35"
													Margin="5,0,0,0"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												刷入
											</Button>
										</StackPanel>
										<StackPanel Margin="0" Orientation="Horizontal">
											<TextBlock Width="50"
													   Margin="8,0,0,0"
													   VerticalAlignment="Center"
													   FontSize="13"
													   Foreground="{DynamicResource SukiLowText}"
													   Text="Product" />
											<TextBox Width="270" Name="FastbootdFile"
													 Text="{Binding FastbootdFile, Mode=TwoWay}"
													 theme:TextBoxExtensions.AddDeleteButton="False"/>
											<Button Height="35"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												文件
											</Button>
											<Button Height="35"
													Margin="5,0,0,0"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												刷入
											</Button>
										</StackPanel>
										<StackPanel Margin="0" Orientation="Horizontal">
											<TextBlock Width="50"
													   Margin="8,0,0,0"
													   VerticalAlignment="Center"
													   FontSize="13"
													   Foreground="{DynamicResource SukiLowText}"
													   Text="Vender" />
											<TextBox Width="270"
													 theme:TextBoxExtensions.AddDeleteButton="False"/>
											<Button Height="35"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												文件
											</Button>
											<Button Height="35"
													Margin="5,0,0,0"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												刷入
											</Button>
										</StackPanel>
										<StackPanel Margin="0" Orientation="Horizontal">
											<TextBlock Width="50"
													   Margin="8,0,0,0"
													   VerticalAlignment="Center"
													   FontSize="13"
													   Foreground="{DynamicResource SukiLowText}"
													   Text="Boot" />
											<TextBox Width="270"
													 theme:TextBoxExtensions.AddDeleteButton="False"/>
											<Button Height="35"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												文件
											</Button>
											<Button Height="35"
													Margin="5,0,0,0"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												刷入
											</Button>
										</StackPanel>
									</StackPanel>
									<StackPanel>
										<StackPanel Margin="0" Orientation="Horizontal">
											<TextBlock Width="70"
													   Margin="8,0,0,0"
													   VerticalAlignment="Center"
													   FontSize="13"
													   Foreground="{DynamicResource SukiLowText}"
													   Text="System_ext" />
											<TextBox Width="250"
													 theme:TextBoxExtensions.AddDeleteButton="False"/>
											<Button Height="35"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												文件
											</Button>
											<Button Height="35"
													Margin="5,0,0,0"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												刷入
											</Button>
										</StackPanel>
										<StackPanel Margin="0" Orientation="Horizontal">
											<TextBlock Width="50"
													   Margin="8,0,0,0"
													   VerticalAlignment="Center"
													   FontSize="13"
													   Foreground="{DynamicResource SukiLowText}"
													   Text="Oblm" />
											<TextBox Width="270"
													 theme:TextBoxExtensions.AddDeleteButton="False"/>
											<Button Height="35"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												文件
											</Button>
											<Button Height="35"
													Margin="5,0,0,0"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												刷入
											</Button>
										</StackPanel>
										<StackPanel Margin="0" Orientation="Horizontal">
											<TextBlock Width="80"
													   Margin="8,0,0,0"
													   VerticalAlignment="Center"
													   FontSize="13"
													   Foreground="{DynamicResource SukiLowText}"
													   Text="Vender_boot" />
											<TextBox Width="240"
													 theme:TextBoxExtensions.AddDeleteButton="False"/>
											<Button Height="35"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												文件
											</Button>
											<Button Height="35"
													Margin="5,0,0,0"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												刷入
											</Button>
										</StackPanel>
										<StackPanel Margin="0" Orientation="Horizontal">
											<TextBlock Width="50"
													   Margin="8,0,0,0"
													   VerticalAlignment="Center"
													   FontSize="13"
													   Foreground="{DynamicResource SukiLowText}"
													   Text="Init_boot" />
											<TextBox Width="270"
													 theme:TextBoxExtensions.AddDeleteButton="False"/>
											<Button Height="35"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												文件
											</Button>
											<Button Height="35"
													Margin="5,0,0,0"
													Padding="10,0,10,0"
													Click="OpenFastbootdFile">
												刷入
											</Button>
										</StackPanel>
									</StackPanel>
								</StackPanel>
							</StackPanel>
						</StackPanel>
					</Grid>
				</suki:BusyArea>
			</suki:GlassCard>

			<!-- Log -->
			<suki:GlassCard Width="790"
							Height="120"
			                Margin="10"
			                VerticalAlignment="Top">
				<StackPanel>
					<TextBlock HorizontalAlignment="Left"
										   VerticalAlignment="Top"
										   FontWeight="DemiBold"
										   Text="预置刷入" />
					<StackPanel Margin="0,10,0,0" Orientation="Horizontal" Spacing="5">
						<Button Height="35"
								Padding="10,0,10,0"
								Click="OpenFastbootdFile">
							文件
						</Button>
						<TextBox Width="280"
								 theme:TextBoxExtensions.AddDeleteButton="False"/>
						<TextBlock Width="50"
								   FontSize="15"
								   VerticalAlignment="Center"
								   Foreground="{DynamicResource SukiLowText}"
								   Text="刷入至" />
						<TextBox Width="280"
								 theme:TextBoxExtensions.AddDeleteButton="False"/>
						<Button Height="35"
								Margin="5,0,0,0"
								Padding="10,0,10,0"
								Click="OpenFastbootdFile">
							刷入
						</Button>
					</StackPanel>
				</StackPanel>
			</suki:GlassCard>

			<!-- Button Panel -->
			<suki:GlassCard Width="150"
							Height="120"
			                Margin="10"
			                VerticalAlignment="Top">
				<StackPanel>
					<StackPanel Orientation="Vertical" Spacing="10">
						<Button Height="35"
								Click="OpenFastbootdFile">
							文件
						</Button>
						<Button Height="35"
								Click="OpenFastbootdFile">
							刷入
						</Button>
					</StackPanel>
				</StackPanel>
			</suki:GlassCard>

			<!-- Log -->
			<suki:GlassCard Width="960"
							Height="340"
			                Margin="10"
			                VerticalAlignment="Top">
				<Grid VerticalAlignment="Top">
					<TextBox Name="CustomizedflashLog" Watermark="日志输出" Margin="0,0,0,0" Height="300" AcceptsReturn="True" IsReadOnly="True"
							 VerticalContentAlignment="Top" TextWrapping="Wrap" BorderThickness="1" CornerRadius="10" />
				</Grid>
			</suki:GlassCard>
		</WrapPanel>
	</ScrollViewer>
</UserControl>